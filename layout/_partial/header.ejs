<h1><a href="<%= config.root %>"><%= config.title %></a></h1>
<div class="menubar">
<nav id="header-menu" class="header-menu">
<a class="toggle-menu">Menu</a>
<%- partial( 'navigation' ) %>
</nav>
<form action="<%= theme.simple_search %>" method="get" class="search">
  <input type="text" name="q" results="0" class="search-field" autocomplete='off'>
  <input type="hidden" name="q" value="site:<%= config.url.replace( /https?:\/\//, '' ) %>">
  <a class="button"></a>
</form>
<%- partial( 'follow_me' ) %>
</div>

<nav class="pagination">
  <% if ( page.prev ) { %>
    <a href="<%= config.root %><%= page.prev_link %>" class="prev"><%= __( 'prev' ) %></a>
  <% } %>
  <% if ( page.next ) { %>
    <a href="<%= config.root %><%= page.next_link %>" class="next"><%= __( 'next' ) %></a>
  <% } %>
  <a class="archives" href="<%= theme.menu.Archives || config.root + 'archives' %>">Blog Archives</a>
</nav>
